{
  "name": "discoui-capacitor",
  "version": "0.0.1",
  "description": "well its disco ui. in android",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "unpkg": "src/index.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    }
  },
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "src/"
  ],
  "author": "cherryhoax",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cherryhoax/DiscoUI-Capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/cherryhoax/DiscoUI-Capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:android && npm run verify:web",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext js",
    "prettier": "prettier \"**/*.{css,html,js,java,json}\" --plugin=prettier-plugin-java",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api DiscoUIPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run embed:css && npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs && vite build",
    "build:android": "npm run build && cd example-app && npm ci && npm run build && npx cap copy android && cd android && ./gradlew assembleDebug",
    "clean": "rimraf ./dist",
    "embed:css": "node scripts/embed-css.js",
    "watch": "vite build --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/docgen": "^0.3.1",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "prettier-plugin-java": "^2.7.7",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.2",
    "swiftlint": "^2.0.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "peerDependencies": {
    "@capacitor/core": ">=8.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "android": {
      "src": "android"
    },
    "web": {
      "src": "src"
    },
    "electron": {
      "src": "src"
    }
  },
  "dependencies": {
    "discoui": "github:cherryhoax/DiscoUI"
  }
}
